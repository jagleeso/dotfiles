#!/bin/bash
source $HOME/.dot_common.sh

local_port=$LOGAN_DASH_PORT
remote_port=$LOGAN_DASH_PORT
remote_dst_node="$REMOTE_LOGAN_NODE"

tunnel_direct \
    $local_port \
    $remote_port \
    $remote_dst_node
NUM_INSTANCES=2
for i in $(seq 1 $((NUM_INSTANCES-1)) ); do
    tunnel_direct \
        $((local_port+1)) \
        $((remote_port+1)) \
        $remote_dst_node
done
