[core]
	excludesfile = ~/.gitignore
	fileMode = false
	autocrlf = input
[diff]
    tool = vimdiff
[difftool]
    prompt = false
[alias]
    d = difftool
    st = status
    co = checkout
    l = log --name-status
    b = branch -v
[color]
	ui = auto
[push]
        # default = simple
	default = matching
